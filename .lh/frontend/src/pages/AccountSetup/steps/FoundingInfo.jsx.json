alue={data.website} onChange={(e)=>update({website:e.target.value})} className=\"w-full border rounded p-2 mt-1\" />\r\n        </div>\r\n      </div>\r\n\r\n      <div className=\"mt-4\">\r\n        <label className=\"text-sm text-gray-600\">Company Vision</label>\r\n        <textarea value={data.vision} onChange={(e)=>update({vision:e.target.value})} className=\"w-full border rounded p-2 mt-1 h-28\" placeholder=\"Tell us about your company vision...\"></textarea>\r\n      </div>\r\n\r\n      <div className=\"mt-6 flex gap-4\">\r\n        <button onClick={prev} className=\"px-4 py-2 border rounded\">Previous</button>\r\n        <button onClick={next} className=\"bg-blue-600 text-white px-4 py-2 rounded\">Save & Next →</button>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n"
